{
  "expo": {
    "name": "Divvy",
    "slug": "divvy",
    "version": "1.0.0",
    "icon": "./assets/app-logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f7a600",
      "imageResizeMode": "contain"
    },
    "scheme": "divvy",
    "newArchEnabled": true,
    "userInterfaceStyle": "light",
    "orientation": "portrait",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/PoppinsSemiBold.ttf",
            "./assets/fonts/PoppinsSemiBoldItalic.ttf",
            "./assets/fonts/Montserrat.ttf"
          ]
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mustafa.dev.divvy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mustafa.dev.divvy"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "38871d99-10cc-4b44-8cb4-5efc6e779f57"
      }
    },
    "owner": "divvy_saas",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/38871d99-10cc-4b44-8cb4-5efc6e779f57"
    }
  }
}
